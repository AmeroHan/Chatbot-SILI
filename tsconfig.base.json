{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": false,
    // "strictNullChecks": false,
    // "noImplicitAny": true,
    "sourceMap": true,
    "declaration": true,
    "resolveJsonModule": true,
    "composite": true,
    "incremental": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@satorijs/element",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment"
  }
}
